                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login successful"
                    },
                    "400": {
                        "description": "Validation error"
                    },
                    "401": {
                        "description": "Invalid email or password"
                    }
                }
            }
        },
        "/users/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "User logout",
                "responses": {
                    "204": {
                        "description": "Logout successful"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/users/edit": {
            "patch": {
                "tags": [
                    "Auth"
                ],
                "summary": "Edit user profile",
                "requestBody": {
                    "required": true,
